<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.seeudin.intentinplisit">

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.SEND_SMS" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".BluetoothActivity" />
        <activity android:name=".WifiActivity" />
        <activity android:name=".BrowserActivity" />
        <activity android:name=".TtsActivity" />
        <activity android:name=".AudioRecorderActivity" />
        <activity android:name=".AlarmActivity" />
        <activity android:name=".EmailActivity" />
        <activity android:name=".HelperFunction" />
        <activity android:name=".CallActivity" />
        <activity android:name=".CallPhoneActivity" />
        <activity android:name=".SmsActivity" />
        <activity android:name=".AudioManagerActivity" />
        <activity android:name=".CameraActivity" />
        <activity android:name=".VideoActivity"></activity>
        <receiver android:name=".AlarmReceiver"></receiver>
    </application>

</manifest>